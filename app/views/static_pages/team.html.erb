<section class="static-page-container container my-5">
    <ul class="estate-list row row-cols-1 row-cols-sm-1 row-cols-md-1 row-cols-lg-3 align-items-stretch align-content-stretch justify-content-center my-3" id="contact-list">
        <li class="mb-5">
            <div class="card h-100">
                <div class="card-image d-flex justify-content-center align-items-center bg-white">
                    <img src=<%= asset_path 'erwan_le_corre.jpg' %> alt="Erwan Le Corre" class="avatar">
                </div>
                <div class="card-body text-center">
                    <h5 class="card-title text-truncate">Erwan Le Corre</h5>
                    <p class="card-text mb-0">Always keep your hovercraft full of eels</p>
                    <%= link_to "https://github.com/Septentrion50", class: "btn-circle mt-3" do %>
                        <i class="fab fa-github"></i>
                    <% end %>
                </div>
            </div>
        </li>
        <li class="mb-5">
            <div class="card h-100">
                <div class="card-image d-flex justify-content-center align-items-center bg-white">
